<% provide :titreH1, "Listing extension_fichiers"%>

<table>
  <thead>
    <tr>
      <th>Extension</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extension_fichiers.each do |extension_fichier| %>
      <tr>
        <td><%= extension_fichier.extension %></td>
        <td><%= link_to 'Show', extension_fichier %></td>
        <td><%= link_to 'Edit', edit_extension_fichier_path(extension_fichier) %></td>
        <td><%= link_to 'Destroy', extension_fichier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extension fichier', new_extension_fichier_path %>
